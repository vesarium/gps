<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,700italic,400,600,700,800') }}" rel="stylesheet"/>
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/font-awesome.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/templatemo-main.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/hero-slider.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/owl-carousel.css') }}" rel="stylesheet"/>



    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/main.js') }}"></script>
    <script src="{{ asset('js/plugins.js') }}"></script>

{% endblock %}
</body>
</html>
